{
  "name": "cadastre.data.gouv.fr",
  "private": true,
  "version": "1.0.0",
  "description": "Landing page pour le cadastre",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "node server",
    "build": "node build/index.mjs && next build",
    "export": "next export",
    "start": "NODE_ENV=production node server",
    "lint": "xo",
    "test": "ava",
    "heroku-postbuild": "next build"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "css-loader": "^6.7.3",
    "debounce": "^1.2.0",
    "express": "^4.17.1",
    "html-react-parser": "^4.0.0",
    "mapbox-gl": "npm:empty-npm-package@^1.0.0",
    "maplibre-gl": "^2.4.0",
    "next": "12",
    "piwik-react-router": "^0.12.1",
    "prop-types": "^15.7.2",
    "react": "17",
    "react-autocomplete": "npm:@webgeodatavore/react-autocomplete@2.0.0",
    "react-autosuggest": "^10.1.0",
    "react-dom": "17",
    "react-feather": "^2.0.9",
    "react-map-gl": "^7.1.8",
    "style-loader": "^3.3.1",
    "underscore.string": "^3.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/eslint-parser": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@next/bundle-analyzer": "^10.0.2",
    "ava": "^6.4.1",
    "babel-plugin-inline-import": "^3.0.0",
    "eslint": "^9.35.0",
    "eslint-config-xo": "^0.46.0",
    "eslint-config-xo-nextjs": "^6.3.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "xo": "^0.60.0"
  },
  "xo": {
    "extends": "xo-nextjs",
    "semicolon": false,
    "space": 2,
    "env": [
      "browser"
    ],
    "rules": {
      "unicorn/catch-error-name": "off",
      "react/jsx-sort-props": "off",
      "react/jsx-child-element-spacing": "off",
      "react/boolean-prop-naming": "off",
      "react/static-property-placement": "off",
      "react/state-in-constructor": "off",
      "react/no-unknown-property": [
        "error",
        {
          "ignore": [
            "jsx",
            "global"
          ]
        }
      ],
      "react/jsx-no-bind": "off"
    }
  }
}
